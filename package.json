    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@elysiajs/eden": "^0.8.1",
    "@elysiajs/swagger": "^0.8.5",
    "@types/bun": "^1.0.4",
    "elysia": "^0.8.10",
    "typescript": "^5.3.3",
    "husky": "^9.0.6"
  },
  "peerDependencies": {
    "elysia": "^0.8.0"
  }
}
